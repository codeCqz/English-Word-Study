<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cqz.dao.SortDao">
    <select id="findSort" resultType="sort">
        select *  from sort
    </select>
    <update id="set" parameterType="string">
        update  words set high='1' where word=#{word}
    </update>
</mapper>